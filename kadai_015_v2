class Human:
  def __init__(self, name, age):
    self.name = name
    self.age = age

  def set_name(self, name):
    self.name = name
  
  def set_age(self,age):
    self.age = age

  def show_name(self):
    print(self.name)

  def show_age(self):
    print(self.age)

man1 = Human("太郎", 30)

man1.show_name()
man1.show_age()
