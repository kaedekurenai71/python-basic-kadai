class Human:
  def __init__(self, name, age):
    self.name = name
    self.age = age
  
  def show_name(self):
    print(self.name)
  
  def check_adult(self):
    if self.age >= 20:
      print("大人である")
    else:
      print("大人ではない")

man1 = Human("太郎", 30)
man2 = Human("次郎", 10)
woman1 = Human("花子", 20)

man1.show_name()
man1.check_adult()
man2.show_name()
man2.check_adult()
woman1.show_name()
woman1.check_adult()
